<div id="youtube-player-{{ url }}" class="youtube-player">
    <div id="video-{{ url }}">
    	<amp-iframe
			width="600"
			height="400"
			sandbox="allow-scripts allow-same-origin allow-presentation"
			layout="responsive"
			frameborder="0"
			src="{{ url }}">
			<amp-img layout="fill" src="/" placeholder></amp-img>
		</amp-iframe>
    </div>
</div>