<div class="gol-partido video-player">
    <div class="image" style="background: linear-gradient(0deg, #000 0%, rgba(0,0,0,0.623687) 22%, #fff 53%);">
        {% if node.hasField('field_image_h') %}
            {{ content.field_image_h }}
        {% elseif node.hasField('field_image') %}
            {{ content.field_image }}
        {% endif %}
    </div>
    <div class="torneo category">{{ content.field_torneo }}</div>
    <div class="title">
        <a href="{{ url }}">{{ label }}</a>
    </div>
</div>