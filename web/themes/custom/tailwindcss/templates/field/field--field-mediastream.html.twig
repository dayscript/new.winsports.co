{% if multiple %}
    <div{{ attributes }}>
        {% for item in items %}
            <div{{ item.attributes }}>
                <iframe src="https://s3.amazonaws.com/WidgetFutbolRed/embed.html?code={{ item.content }}" width="100%" height="314" frameborder="0" allow="autoplay; fullscreen" allowfullscreen="" allowscriptaccess="always" scrolling="no" style="width: 100%; height: 314.438px;"></iframe>
            </div>
        {% endfor %}
    </div>
{% else %}
    {% for item in items %}
        <div{{ attributes }}>
            <iframe src="https://s3.amazonaws.com/WidgetFutbolRed/embed.html?code={{ item.content }}" width="100%" height="314" frameborder="0" allow="autoplay; fullscreen" allowfullscreen="" allowscriptaccess="always" scrolling="no" style="width: 100%; height: 314.438px;"></iframe>
        </div>
    {% endfor %}
{% endif %}